{
  "name": "@figliolia/react-animated-number",
  "version": "1.0.1",
  "description": "A react component that animates between numeric values",
  "author": "Alex Figliolia",
  "scripts": {
    "transpile": "ts-packager -e src",
    "lint": "tsx ci/commands/Lint.ts",
    "test": "tsx ci/commands/Test.ts",
    "copy-files": "copyfiles -u 1 src/**/*.css dist/cjs && copyfiles -u 1 src/**/*.css dist/mjs",
    "build": "yarn transpile && yarn copy-files"
  },
  "files": [
    "dist",
    "src"
  ],
  "browserslist": [
    "defaults",
    "not dead"
  ],
  "homepage": "https://github.com/alexfigliolia/react-animated-number#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexfigliolia/react-animated-number.git"
  },
  "keywords": [
    "react",
    "animation",
    "animated-number"
  ],
  "dependencies": {
    "@figliolia/classnames": "^1.0.1",
    "@figliolia/react-hooks": "^1.2.0"
  },
  "peerDependencies": {
    "@figliolia/classnames": "^1.0.1",
    "@figliolia/react-hooks": "^1.2.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@figliolia/child-process": "^1.0.1",
    "@figliolia/ts-packager": "^1.0.9",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.11.19",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}